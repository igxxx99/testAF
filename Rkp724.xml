                    <pb xml:id="Rkp724_fol_04" n="04"
                        facs="https://arfi.kul.pl/Scaler/iiif/BUKUL%21Rkp724%21Image00004/full/full/0/default.jpg"/>
                    
                    <pb xml:id="Rkp724_fol_1r" n="1r"
                        facs="https://arfi.kul.pl/Scaler/iiif/BUKUL%21Rkp724%21Image00005/full/full/0/default.jpg"/>

                    <l n="1">
                        <lb xml:id="Rkp724_lb_1r_01" facs="#Rkp724_line_1r_01" n="1"/>
                        <choice>
                            <orig>Sonet. <add place="right" hand="Mickiewicz">(s
                                Petrarcha)</add></orig>
                            <reg>Sonet. Z Petrarka</reg>
                        </choice>
                    </l>

                    <l n="2">
                        <lb xml:id="Rkp724_lb_1r_02" facs="#Rkp724_line_1r_02" n="2"/>
                        <choice>
                            <orig>Przypomnienie</orig>
                            <reg>Przypomnienie</reg>
                        </choice>
                    </l>

                    <l n="3">
                        <lb xml:id="Rkp724_lb_1r_03" n="3" rend="empty"/></l>


                    <l n="4"><lb xml:id="Rkp724_lb_1r_04" facs="#Rkp724_line_1r_04" n="4"/>
                        <choice>
                            <orig><del rend="strikethrough">Julio</del><add place="above"
                                    hand="Czeczot">Lauro li</add> czyż te piękne wieków naszych
                                lata</orig>
                            <reg>Lauro, czyliż te piękne wieków naszych lata</reg>
                        </choice></l>
                    <l n="5"><lb xml:id="Rkp724_lb_1r_05" facs="#Rkp724_line_1r_05" n="5"/>
                        <choice>
                            <orig>Jeszcze się kiedy twojéj malują pamięci?</orig>
                            <reg>Jeszcze się kiedy twojéj malują pamięci,</reg>
                        </choice></l>
                    <l n="6"><lb xml:id="Rkp724_lb_1r_06" facs="#Rkp724_line_1r_06" n="6"/>
                        <choice>
                            <orig>Kiedyśmy sami <add place="above" hand="Mickiewicz">tylko</add> i
                                sobą <del rend="above">jedynie</del> zajęci,</orig>
                            <reg>Kiedyśmy sami tylko i sobą zajęci</reg>
                        </choice></l>
                    <l n="7"><lb xml:id="Rkp724_lb_1r_07" facs="#Rkp724_line_1r_07" n="7"/>
                        <choice>
                            <orig>Dbać nie chcieli o resztę obcego nam świata.</orig>
                            <reg>Dbać nie chcieli o resztę obcego nam świata?</reg>
                        </choice></l>

                    <l n="8"><lb xml:id="Rkp724_lb_1r_08" n="8" rend="empty"/></l>


                    <l n="9"><lb xml:id="Rkp724_lb_1r_09" facs="#Rkp724_line_1r_09" n="9"/>
                        <choice>
                            <orig>Chłodnik, co się zielonym jaźminem wyplata,</orig>
                            <reg>Chłodnik, co się zielonym jaśminem wyplata,</reg>
                        </choice></l>
                    <l n="10"><lb xml:id="Rkp724_lb_1r_10" facs="#Rkp724_line_1r_10" n="10"/>
                        <choice>
                            <orig>Strumień, co z miłym szmerem po łące się kręci;</orig>
                            <reg>Strumień, co z miłym szmerem po łące się kręci,</reg>
                        </choice></l>
                    <l n="11"><lb xml:id="Rkp724_lb_1r_11" facs="#Rkp724_line_1r_11" n="11"/>
                        <choice>
                            <orig>Tam nas często wzajemne tłómaczących chęci,</orig>
                            <reg>Tam nas często wzajemne tłumaczących chęci</reg>
                        </choice></l>
                    <l n="12"><lb xml:id="Rkp724_lb_1r_12" facs="#Rkp724_line_1r_12" n="12"/>
                        <choice>
                            <orig>Późnéj nocy miłośna osłoniła szata.</orig>
                            <reg>Późnéj nocy miłośna osłoniła szata.</reg>
                        </choice></l>


                    <l n="13"><lb xml:id="Rkp724_lb_1r_13" n="13" rend="empty"/></l>

                    <l n="14"><lb xml:id="Rkp724_lb_1r_14" facs="#Rkp724_line_1r_14" n="14"/>
                        <choice>
                            <orig>A xiężyc s pod bladego wyjrzawszy obłoku</orig>
                            <reg>A księżyc spod bladego wyjrzawszy obłoku,</reg>
                        </choice></l>
                    <l n="15">
                        <lb xml:id="Rkp724_lb_1r_15" facs="#Rkp724_line_1r_15" n="15"/>
                        <choice>
                            <orig>Sniéżne piersi i złote rozświecał pierścienie,</orig>
                            <reg>Śniéżne piersi i złote rozświecał pierścienie,</reg>
                        </choice></l>
                    <l n="16"><lb xml:id="Rkp724_lb_1r_16" facs="#Rkp724_line_1r_16" n="16"/>
                        <choice>
                            <orig>Boskiego wdziękóm twoim przydając uroku.</orig>
                            <reg>Boskiego wdziękóm twoim przydając uroku.</reg>
                        </choice></l>

                    <l n="17"><lb xml:id="Rkp724_lb_1r_17" n="17" rend="empty"/></l>

                    <l n="18"><lb xml:id="Rkp724_lb_1r_18" facs="#Rkp724_line_1r_18" n="18"/>
                        <choice>
                            <orig>Wtenczas serce porywa słodkie zachwycenie,</orig>
                            <reg>Wtenczas serce porywa słodkie zachwycenie,</reg>
                        </choice></l>

                    <l n="19"><lb xml:id="Rkp724_lb_1r_19" facs="#Rkp724_line_1r_19" n="19"/>
                        <choice>
                            <orig>Usta się spotykają, oko <del rend="strikethrough">bliskie</del>
                                <add place="above" hand="Mickiewicz">ginie</add> oku</orig>
                            <reg>Usta się spotykają, oko ginie [w] oku,</reg>
                        </choice></l>
                    <l n="20">
                        <lb xml:id="Rkp724_lb_1r_20" facs="#Rkp724_line_1r_20" n="20"/>
                        <choice>
                            <orig>Łza ze łzą, i z westchnieniem miesza się</orig>
                            <reg>Łza ze łzą i z westchnieniem miesza się</reg>
                        </choice></l>
                    <l n="21"><lb xml:id="Rkp724_lb_1r_21" facs="#Rkp724_line_1r_21" n="21"/>
                        <choice>
                            <orig>westchnienie!</orig>
                            <reg/>
                        </choice></l>


                </div>

                <div n="Amalia" subtype="edycja_utworu" type="wiersz" xml:id="Amalia">

                    <pb xml:id="Rkp723_fol_2r" n="2r"
                        facs="https://arfi.kul.pl/Scaler/iiif/BUKUL%21Rkp724%21Image00006/full/full/0/default.jpg"/>

                    <l n="1"><lb xml:id="Rkp724_lb_2r_01" facs="#Rkp724_line_2r_01" n="1"/>
                        <choice>
                            <orig>amalia (s Szyllera).</orig>
                            <reg>Amalia. Z Schillera</reg>
                        </choice></l>

                    <l n="2">
                        <lb xml:id="Rkp724_lb_2r_02" n="2" rend="empty"/></l>

                    <l n="3"><lb xml:id="Rkp724_lb_2r_03" facs="#Rkp724_line_2r_03" n="3"/>
                        <choice>
                            <orig>Naypięknieysza iako anioł z raiu</orig>
                            <reg>Najpięknieysza jako anioł z raju,</reg>
                        </choice></l>

                    <l n="4"><lb xml:id="Rkp724_lb_2r_04" facs="#Rkp724_line_2r_04" n="4"/>
                        <choice>
                            <orig>Naypięknieysza nad wszystkie dziewica</orig>
                            <reg>Najpiękniejsza nad wszystkie dziewica,</reg>
                        </choice></l>

                    <l n="5"><lb xml:id="Rkp724_lb_2r_05" facs="#Rkp724_line_2r_05" n="5"/>
                        <choice>
                            <orig>Wzrok iey luby iako słonce w maiu</orig>
                            <reg>Wzrok jej luby jako słońce w maju,</reg>
                        </choice></l>

                    <l n="6"><lb xml:id="Rkp724_lb_2r_06" facs="#Rkp724_line_2r_06" n="6"/>
                        <choice>
                            <orig>Odstrzelone od modrych wod lica,</orig>
                            <reg>Odstrzelone od modrych wod lica.</reg>
                        </choice></l>

                    <l n="7">
                        <lb xml:id="Rkp724_lb_2r_07" n="7" rend="empty"/></l>

                    <l n="8"><lb xml:id="Rkp724_lb_2r_08" facs="#Rkp724_line_2r_08" n="8"/>
                        <choice>
                            <orig>Pocałunek iéy ach nektar boski</orig>
                            <reg>Pocałunek jéj, ach, nektar boski,</reg>
                        </choice></l>

                    <l n="9"><lb xml:id="Rkp724_lb_2r_09" facs="#Rkp724_line_2r_09" n="9"/>
                        <choice>
                            <orig>Jako płomień gubisię w płomieniu,</orig>
                            <reg>Jako płomień gubi się w płomieniu,</reg>
                        </choice></l>

                    <l n="10"><lb xml:id="Rkp724_lb_2r_10" facs="#Rkp724_line_2r_10" n="10"/>
                        <choice>
                            <orig>Jak dwóch fletów zeniących się głoski,</orig>
                            <reg>Jak dwóch fletów żeniących się głoski</reg>
                        </choice></l>

                    <l n="11"><lb xml:id="Rkp724_lb_2r_11" facs="#Rkp724_line_2r_11" n="11"/>
                        <choice>
                            <orig>Ku Rayskiemu nastroiona pieniu.</orig>
                            <reg>Ku rajskiemu nastrojona pieniu.</reg>
                        </choice></l>

                    <l n="12">
                        <lb xml:id="Rkp724_lb_2r_12" n="12" rend="empty"/></l>

                    <l n="13"><lb xml:id="Rkp724_lb_2r_13" facs="#Rkp724_line_2r_13" n="13"/>
                        <choice>
                            <orig>Serca s sercem zbiega zlatuiesię sciska,</orig>
                            <reg>Serce z sercem zbiega, zlatuje się, ściska,</reg>
                        </choice></l>

                    <l n="14"><lb xml:id="Rkp724_lb_2r_14" facs="#Rkp724_line_2r_14" n="14"/>
                        <choice>
                            <orig>Lico z licem zbiegasię drzy pali,</orig>
                            <reg>Lico z licem zbiega się, drży, pali,</reg>
                        </choice></l>

                    <l n="15"><lb xml:id="Rkp724_lb_2r_15" facs="#Rkp724_line_2r_15" n="15"/>
                        <choice>
                            <orig>Dusza w duszy tonie - Zie<del rend="overstrike">b</del>mia niebu
                                pryska,</orig>
                            <reg>Dusza w duszy tonie - Ziemia niebu pryska</reg>
                        </choice></l>

                    <l n="16"><lb xml:id="Rkp724_lb_2r_16" facs="#Rkp724_line_2r_16" n="16"/>
                        <choice>
                            <orig>Jak w stopioney około nas fali.</orig>
                            <reg>Jak w stopionej około nas fali.</reg>
                        </choice></l>

                    <l n="17">
                        <lb xml:id="Rkp724_lb_2r_17" n="17" rend="empty"/></l>

                    <l n="18"><lb xml:id="Rkp724_lb_2r_18" facs="#Rkp724_line_2r_18" n="18"/>
                        <choice>
                            <orig>Niemasz Jey - daremnie ach daremnie,</orig>
                            <reg>Nie masz Jej - daremnie, ach, daremnie</reg>
                        </choice></l>

                    <l n="19"><lb xml:id="Rkp724_lb_2r_19" facs="#Rkp724_line_2r_19" n="19"/>
                        <choice>
                            <orig>Myslą scigam za pamiątek cieniem</orig>
                            <reg>Myślą ścigam za pamiątek cieniem.</reg>
                        </choice></l>

                    <l n="20"><lb xml:id="Rkp724_lb_2r_20" facs="#Rkp724_line_2r_20" n="20"/>
                        <choice>
                            <orig>Niemasz iey, i wszelka roskosz zemnie,</orig>
                            <reg>Nie masz jej i wszelka rozkosz ze mnie</reg>
                        </choice></l>

                    <l n="21"><lb xml:id="Rkp724_lb_2r_21" facs="#Rkp724_line_2r_21" n="21"/>
                        <choice>
                            <orig>Jako lekki dym uszła z westchnieniem –</orig>
                            <reg>Jako lekki dym uszła z westchnieniem.</reg>
                        </choice></l>


                </div>

                <div n="Si Qua" subtype="edycja_utworu" type="wiersz" xml:id="Si_qua_meis">

                    <pb xml:id="Rkp723_fol_2v" n="2v"
                        facs="https://arfi.kul.pl/Scaler/iiif/BUKUL%21Rkp723%21Image00006/full/full/0/default.jpg"/>

                    <l n="1"><lb xml:id="Rkp723_lb_2v_01" facs="#Rkp723_line_2v_01" n="1"/>
                        <choice>
                            <orig><hi rend="underline">Ovidii Lib. IV Elegia I.</hi> Si qua
                                meis</orig>
                            <reg>Ovidii Lib. IV Elegia I</reg>
                        </choice></l>

                    <l n="2">
                        <lb xml:id="Rkp723_lb_2v_02" n="2" rend="empty"/></l>

                    <l n="3"><lb xml:id="Rkp723_lb_2v_03" facs="#Rkp723_line_2v_03" n="3"/>
                        <choice>
                            <orig>Jeśli błędy spotkaią, błędów pieśni wiele</orig>
                            <reg>Jeśli błędy spotkają błędów pieśni wiele,</reg>
                        </choice></l>

                    <l n="4"><lb xml:id="Rkp723_lb_2v_04" facs="#Rkp723_line_2v_04" n="4"/>
                        <choice>
                            <orig>przez wzgląd słuszny, daruycie błędom przyiaciele,</orig>
                            <reg>przez wzgląd słuszny darujcie błędom, przyiaciele.</reg>
                        </choice></l>

                    <l n="5"><lb xml:id="Rkp723_lb_2v_05" facs="#Rkp723_line_2v_05" n="5"/>
                        <choice>
                            <orig>Za pociechą wygnaniec nie za sławą goni.</orig>
                            <reg>Za pociechą wygnaniec, nie za sławą goni,</reg>
                        </choice></l>

                    <l n="6"><lb xml:id="Rkp723_lb_2v_06" facs="#Rkp723_line_2v_06" n="6"/>
                        <choice>
                            <orig>piesń dzwiga myśli w gorzkiey grążące się toni,</orig>
                            <reg>Piesń dżwiga myśli w gorzkiej grążące się toni.</reg>
                        </choice></l>

                    <l n="7"><lb xml:id="Rkp723_lb_2v_07" facs="#Rkp723_line_2v_07" n="7"/>
                        <choice>
                            <orig>Tak pieie kopacz więzień i pieniem bezkształtu.</orig>
                            <reg>Tak pieje kopacz więzień i pieniem bezkształtu</reg>
                        </choice></l>

                    <l n="8"><lb xml:id="Rkp723_lb_2v_08" facs="#Rkp723_line_2v_08" n="8"/>
                        <choice>
                            <orig>Uymie trudu robocie, uymie troskom gwałtu,</orig>
                            <reg>Ujmie trudu robocie, ujmie troskom gwałtu.</reg>
                        </choice></l>

                    <l n="9"><lb xml:id="Rkp723_lb_2v_09" facs="#Rkp723_line_2v_09" n="9"/>
                        <choice>
                            <orig>Gdy Flisa tratwy z nurtem kłucące się nagną</orig>
                            <reg>Gdy Flisa tratwy z nurtem kłócące się nagną,</reg>
                        </choice></l>

                    <l n="10"><lb xml:id="Rkp723_lb_2v_10" facs="#Rkp723_line_2v_10" n="10"/>
                        <choice>
                            <orig>pieje Flis, stopy wlipkie zatapiaiąc bagno</orig>
                            <reg>Pieje Flis, stopy w lipkie zatapiając bagno.</reg>
                        </choice></l>

                    <l n="11"><lb xml:id="Rkp723_lb_2v_11" facs="#Rkp723_line_2v_11" n="11"/>
                        <choice>
                            <orig>I nędzny lud łańcuchém ku wiosłu przygięty,</orig>
                            <reg>I nędzny lud, łańcuchém ku wiosłu przygięty,</reg>
                        </choice></l>

                    <l n="12"><lb xml:id="Rkp723_lb_2v_12" facs="#Rkp723_line_2v_12" n="12"/>
                        <choice>
                            <orig>W takt wiosłem robi, morskie wtakt kraje odmęty.</orig>
                            <reg>W takt wiosłem robi, morskie w takt kraje odmęty.</reg>
                        </choice></l>

                    <l n="13"><lb xml:id="Rkp723_lb_2v_13" facs="#Rkp723_line_2v_13" n="13"/>
                        <choice>
                            <orig>Ledwie na kiju wsparł się, lub usiadł na darni,</orig>
                            <reg>Ledwie na kiju wsparł się lub usiadł na darni,</reg>
                        </choice></l>

                    <l n="14"><lb xml:id="Rkp723_lb_2v_14" facs="#Rkp723_line_2v_14" n="14"/>
                        <choice>
                            <orig>Spocony pasterz, słomką noci swéy owczarni</orig>
                            <reg>Spocony pasterz słomką noci swéj owczarni.</reg>
                        </choice></l>

                    <l n="15"><lb xml:id="Rkp723_lb_2v_15" facs="#Rkp723_line_2v_15" n="15"/>
                        <choice>
                            <orig>Kiedy się to pieśń ciągnie, to zwazona przędza</orig>
                            <reg>Kiedy się to pieśń ciągnie, to zważona przędza,</reg>
                        </choice></l>

                    <l n="16"><lb xml:id="Rkp723_lb_2v_16" facs="#Rkp723_line_2v_16" n="16"/>
                        <choice>
                            <orig>Łatwy trud słuzebnicy czas się miley spędza.</orig>
                            <reg>Łatwy trud służebnicy, czas się milej spędza.</reg>
                        </choice></l>

                    <l n="17"><lb xml:id="Rkp723_lb_2v_17" facs="#Rkp723_line_2v_17" n="17"/>
                        <orig>. . . . . . . . .</orig></l>

