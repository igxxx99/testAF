    <text xml:id="Rkp732_text_OdaDoMlodosci" n="Rkp 732">
        <body>
            <div n="Rkp732" subtype="edycja_dokumentu" xml:id="sygn_Rkp732">
                
                <pb rend="2col_layout" xml:id="Rkp732_fol_01" n="01"
                    facs="https://arfi.kul.pl/Scaler/iiif/BUKUL%21Rkp732%21Image00015/full/full/0/default.jpg"/>

                <div n="Oda do młodości" subtype="edycja_utworu" type="wiersz"
                    xml:id="OdaDoMlodosci">

                    <pb rend="2col_layout" xml:id="Rkp732_fol_1r" n="1r"
                        facs="https://arfi.kul.pl/Scaler/iiif/BUKUL%21Rkp732%21Image00016/full/full/0/default.jpg"/>
                    
                        <l n="1">
                        <lb xml:id="Rkp732_lb_1r_01" facs="#Rkp732_line_1r_01" n="1"/>
                        <choice>
                            <orig>Bez serc, bez ducha, tu skieletow ludy!</orig>
                            <reg>Bez serc, bez ducha, tu <corr>sz</corr>kieletow ludy!</reg>
                        </choice>
                    </l>
                    <l n="2">
                        <lb xml:id="Rkp732_lb_1r_02" facs="#Rkp732_line_1r_02" n="2"/>
                        <choice>
                            <orig>Młodosci poday mi skrzydła,</orig>
                            <reg>Młodości, podaj mi skrzydła,</reg>
                        </choice>
                    </l>
                    <l n="3">
                        <lb xml:id="Rkp732_lb_1r_03" facs="#Rkp732_line_1r_03" n="3"/>
                        <choice>
                            <orig>A nad martwym wzlecę światem</orig>
                            <reg>A nad martwym wzlecę światem</reg>
                        </choice>
                    </l>
                    <l n="4">
                        <lb xml:id="Rkp732_lb_1r_04" facs="#Rkp732_line_1r_04" n="4"/>
                        <choice>
                            <orig>W Rayską krainę ułudy</orig>
                            <reg>W rajską krainę ułudy,</reg>
                        </choice>
                    </l>
                    <l n="5">
                        <lb xml:id="Rkp732_lb_1r_05" facs="#Rkp732_line_1r_05" n="5"/>
                        <choice>
                            <orig>Kędy zapał tworzy cudy</orig>
                            <reg>Kędy zapał tworzy cudy,</reg>
                        </choice>
                    </l>
                    <l n="6">
                        <lb xml:id="Rkp732_lb_1r_06" facs="#Rkp732_line_1r_06" n="6"/>
                        <choice>
                            <orig>Nowość potrząsa kwiatem</orig>
                            <reg>Nowość potrząsa kwiatem</reg>
                        </choice>
                    </l>
                    <l n="7">
                        <lb xml:id="Rkp732_lb_1r_07" facs="#Rkp732_line_1r_07" n="7"/>
                        <choice>
                            <orig>J obleka nadziei złote malowidła.</orig>
                            <reg>I obleka nadziei złote malowidła.</reg>
                        </choice>
                    </l>
                    <l n="8">
                        <lb xml:id="Rkp732_lb_1r_08" rend="empty" n="8"/>
                    </l>
                    <l n="9">
                        <lb xml:id="Rkp732_lb_1r_09" facs="#Rkp732_line_1r_09" n="9"/>
                        <choice>
                            <orig>Niechay kogo wiek zamroczy</orig>
                            <reg>Niechaj kogo wiek zamroczy,</reg>
                        </choice>
                    </l>
                    <l n="10">
                        <lb xml:id="Rkp732_lb_1r_10" facs="#Rkp732_line_1r_10" n="10"/>
                        <choice>
                            <orig>Chylac ku ziemi poradlone czoło</orig>
                            <reg>Chyląc ku ziemi poradlone czoło,</reg>
                        </choice>
                    </l>
                    <l n="11">
                        <lb xml:id="Rkp732_lb_1r_11" facs="#Rkp732_line_1r_11" n="11"/>
                        <choice>
                            <orig>Taki widzi świata koło</orig>
                            <reg>Taki<corr>e</corr> widzi świata koło,</reg>
